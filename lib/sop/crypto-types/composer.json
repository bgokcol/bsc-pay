	"homepage": "https://github.com/sop/crypto-types",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"cryptography",
		"asn1",
		"algorithm identifier",
		"rsa",
		"ec",
		"public key",
		"private key",
		"signature"
	],
	"authors": [
		{
			"name": "Joni Eskelinen",
			"email": "jonieske@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.0",
		"ext-gmp": "*",
		"sop/asn1": "^3.2.0",
		"sop/crypto-encoding": "^0.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.4"
	},
	"autoload": {
		"psr-4": {
			"Sop\\CryptoTypes\\" : "lib/CryptoTypes"
		}
	}
